#. card_title
msgctxt "477808"
msgid "The Three Seasons"
msgstr ""

#. internal_ability, printed_ability, type_enchantment
msgctxt "477809"
msgid "I — Mill three cards."
msgstr "I — Zmiel trzy karty."

